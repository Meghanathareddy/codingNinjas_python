    "def printSubs(str, output):\n",
    "    if len(str) == 0:\n",
    "        print(output)\n",
    "        return\n",
    "        \n",
    "    #don't include 0th char\n",
    "    printSubs(str[1:],output)\n",
    "\n",
    "    #include 0th char\n",
    "    printSubs(str[1:],output+str[0])\n"
