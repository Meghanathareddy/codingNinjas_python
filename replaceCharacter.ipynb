    "def replace(str, char1, char2):\n",
    "    newStr = \" \"\n",
    "    for char in str:\n",
    "        if (char == char1):\n",
    "            newStr += char2\n",
    "        else:\n",
    "            newStr += char\n",
    "    return newStr\n",
    "\n",
